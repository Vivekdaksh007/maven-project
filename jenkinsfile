pipeline
{
    agent any
    stages
    {
        stage('SCM checkout')
         { steps { git branch: 'master' , url: 'https://github.com/Vivekdaksh007/maven-project'  } }

        stage('build the code')
        { steps { withMaven(jdk: 'JAVA_HOME', maven: 'MAVEN_HOME') 
         { sh 'mvn package'}
         
         }}
      

    }

}