pipeline
{
    agent any
    stages
    {
        stage('SCM checkout')
         { 
         steps { git branch: 'master' , url: 'https://github.com/Vivekdaksh007/maven-project'  } 
         }
        
        stage('code built and test cases')
        {
          parallel
           {
            stage('executing test')
            { steps { withMaven(jdk: 'JAVA_HOME', maven: 'MAVEN_HOME') 
                    {sh 'mvn test'}
                    }
            }
            
            stage('code build')
            { steps { withMaven(jdk: 'JAVA_HOME', maven: 'MAVEN_HOME') 
                    {sh 'mvn package'}
                    }
            }
           }
         }
      }
  }
